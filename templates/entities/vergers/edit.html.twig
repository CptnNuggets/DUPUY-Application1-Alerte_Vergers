{% extends 'layouts/base.html.twig' %}

{% set pagetitle = ('Modification du verger : ' ~ verger.idVerger)  %}

{% block title pagetitle %}

{% block body %}

<div class='container' >
    <h1>{{ pagetitle }}</h1>
</div>

<div class='container' >
    {{ form_start(form) }}
        {{form_widget(form)}}
        <input type="submit" value="Create">
    {{ form_rest(form) }}
    {{ form_end(form) }}
</div>
{% endblock %}
