{% extends 'layouts/base.html.twig' %}

{% block title %}Import d'une configuration FieldClimate depuis l'API{% endblock %}




{% block body %}

<div class='container' >
    <div class='text-center'>
        <h1>Nommer la nouvelle station</h1>
    </div>
</div>

<div style="height:20px"></div>

<div class='container w-50 center border border-secondary rounded' >
