        {{form_widget(form)}}
        <div class="text-center">
            <input type="submit" value="Valider l'identification" class="btn btn-primary">
        </div>
    {{ form_rest(form) }}
    {{ form_end(form) }}
    <br>
</div>
{% endblock %}